        <link rel="stylesheet" type="text/css" href="{{ asset('assets/css/site.css') }}">{# en.twig cette forme du href est conseillé ATTENTION site.css = style.css de l'ancien projet#}
        
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('assets/css/page-admin.css') }}">
        {% endblock %}
    </head>
    <body>
    
    <!-- Header Area Start -->
    <header class="header-area">
        <!-- Main Header Start -->
        <div class="main-header-area">
            <div class="classy-nav-container breakpoint-off">
                <div class="container">
                    <!-- Classy Menu -->
                    <nav class="classy-navbar justify-content-between" id="alimeNav">

                        <!-- Logo -->
                        <a class="nav-brand" href="{{ path('public') }}"><img src="{{ asset('assets/img/core-img/logo33.png') }}" alt=""></a>

                        <!-- Navbar Toggler -->
                        <div class="classy-navbar-toggler">
                            <span class="navbarToggler"><span></span><span></span><span></span></span>
                        </div>

                        <!-- Menu -->
                        <div class="classy-menu">
                            <!-- Menu Close Button -->
                            <div class="classycloseIcon">
                                <div class="cross-wrap"><span class="top"></span><span class="bottom"></span></div>
                            </div>
                            <!-- Nav Start -->
                            <div class="classynav">
                                <ul id="nav">
                                    <li class="active"><a href="{{ path('public') }}">Accueil</a></li>
                                    <li><a href="#">Pages</a>
                                        <ul class="dropdown">
                                            
                                            <li><a href="{{ path('admin') }}">Section VIP</a></li>

                                            <li><a href="{{ path('applivolleywood') }}">Appli-Volleywood</a></li>

                                            <li><a href="{{ path('contact') }}">Contact</a></li>

                                            <li><a href="{{ path('faireundon') }}">Faire un don</a></li>
                                            
                                            <li><a href="{{ path('jeuxvideo') }}">5 min à perdre ?</a></li>
                                        </ul>
                                    </li>
                                    <li><a href="{{ path('nosProduits') }}">Boutique</a></li>
                                    <li><a href="{{ path('galerie') }}">Galerie</a></li>
                                    <li><a href="{{ path('blog') }}">Blog</a></li>
                                    {% if not app.user %}
                                    <li><a href="{{ path('inscription') }}">S'inscrire</a></li>
                                    {% else %}
                                    <li><a class="icon login" href="#"> Bonjour {{ app.user.pseudo }}</a></li>
                                    {% endif %}
                                    {% if not app.user %}
                                    <li><a href="{{ path('app_login') }}">Connexion</a></li>
                                    {% else %}
                                    <li><a href="{{ path('app_logout') }}">Déconnexion</a></li>
                                    {% endif %}
                                </ul>
                            </div>
                            <!-- Nav End -->
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- Header Area End -->

        {% block body %}
        {% endblock %}


    <!-- Footer Area Start -->
    <footer class="footer-area">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="footer-content d-flex align-items-center justify-content-between">
                        <!-- Copywrite Text -->
                        <div class="copywrite-text">
                            <p>
Copyright &copy;<script>document.write(new Date().getFullYear());</script> par F.DENIAU, P.JUILLAN, J.NERI, A.VARESE </a>
</p>
                        
                        <!-- Social Info -->
